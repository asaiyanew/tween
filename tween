wait(1000000000000000000000000)

local function teleportToTarget()
    -- Tween to Specific CFrame Position/Rotation
    local targetCFrame = CFrame.new(
        1346.18188, 8914.84277, -13401.5059, 0.76193881, 2.71736056e-08, 0.64764899, 1.34852094e-08, 1, -5.78222306e-08, -0.64764899, 5.27906856e-08, 0.76193881
    )

    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    -- Check if TweenService is available
    if game:GetService("TweenService") then
        local tweenService = game:GetService("TweenService")
        
        -- Create tween info
        local tweenInfo = TweenInfo.new(
            5, -- Duration (seconds)
            Enum.EasingStyle.Linear, -- Easing style
            Enum.EasingDirection.Out -- Easing direction
        )
        
        -- Create tween
        local tween = tweenService:Create(humanoidRootPart, tweenInfo, {CFrame = targetCFrame})
        
        -- Play tween
        tween:Play()
        
        print("Tweening to position: " .. tostring(targetCFrame.Position))
        print("Rotation applied. Duration: 5 seconds")
        
        -- Optional: Wait for tween to complete
        tween.Completed:Wait()
        print("Tween complete!")
    else
        -- Fallback: Instantly teleport if TweenService isn't available
        humanoidRootPart.CFrame = targetCFrame
        print("Instantly teleported to position: " .. tostring(targetCFrame.Position))
    end
end

-- Run immediately once
teleportToTarget()

-- Use spawn to create a separate thread for the loop
spawn(function()
    while true do
        wait(15) -- Wait 15 seconds
        teleportToTarget() -- Run the teleport function
    end
end)

print("Teleport loop started. Will run every 15 seconds.")
